proc testproc6(poly p, poly q){
  matrix m[3][10];
  int i;
  int j;
  for(i=1;i<=3;i++){
    for(j=1;j<=10;j++){
	m[i,j]=p^i*q^j;
    }
  }

  return(m);
}
