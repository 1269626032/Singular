proc msetring(int ch, int nvars, string ordering)
{
  execute "ring r = " + string(ch) + ",x(1.." + string(nvars) + "), " +
  ordering + ";"; 
}
msetring(0,2,"dp");
;
proc msetring(int ch, int nv, string ordering)
{
  execute "ring r = " + string(ch) + ",x(1.." + string(nv) + "), " +
  ordering + ";"; 
}
msetring(0,2,"dp");
basering;
proc msetring(int ch, int nv, string ordering)
{
  execute "ring r = " + string(ch) + ",x(1.." + string(nv) + "), " +
  ordering + ";"; 
  keepring r;
}
msetring(0,2,"dp");
basering;
